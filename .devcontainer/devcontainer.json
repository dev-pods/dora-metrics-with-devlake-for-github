{
  "name": "dora-metrics-with-devlake-for-github",
  "forwardPorts": [
    3002,
    4000,
    8080,
    3306
  ],
  "portsAttributes": {
    "3002": {
      "label": "Grafana",
      "onAutoForward": "silent"
    },
    "8080": {
      "label": "DevLake",
      "onAutoForward": "silent"
    },
    "4000": {
      "label": "DevLake Config UI",
      "onAutoForward": "silent"
    },
    "3306": {
      "label": "MySQL",
      "onAutoForward": "silent"
    }
  },
  "initializeCommand": "cp -n .env.example .env 2>/dev/null || true",
  "postCreateCommand": "docker compose -f 'docker-compose.yml' up -d --build",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "terminal.integrated.profiles.linux": {
          "bash": {
            "path": "/bin/bash"
          }
        }
      },
      "extensions": [
        "ms-azuretools.vscode-docker",
        "eamodio.gitlens",
        "ms-python.python",
        "yzhang.markdown-all-in-one",
        "bierner.github-markdown-preview"
      ]
    }
  }
}
